{
    "name": "edulazaro/larascraper",
    "description": "Scraper for Laravel",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": ">=10.0",
        "symfony/dom-crawler": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "EduLazaro\\Larascraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EduLazaro\\Larascraper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edu Lazaro",
            "email": "edu@edulazaro.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "EduLazaro\\Larascraper\\LarascraperServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.34"
    }
}
